{% block css %}
      <link rel="stylesheet" href="{{ asset('css/login.css') }}" />
{% endblock css %}
 <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4">
            <h1 class="text-center login-title">Se logue no sistema</h1>
            <div class="account-wall">
                <img class="profile-img" src="https://lh5.googleusercontent.com/-b0-k99FZlyE/AAAAAAAAAAI/AAAAAAAAAAA/eu7opA4byxI/photo.jpg?sz=120"
                    alt="">
                <form class="form-signin" action="{{ path("fos_user_security_check") }}" method="post">
                    <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />    
                    
                    <input type="text" class="form-control"  id="username" name="_username" value="{{ last_username }}" placeholder="UsuÃ¡rio" required autofocus>
                    <input type="password" class="form-control" placeholder="Senha" id="password" name="_password" required>
                    
                    <button class="btn btn-lg btn-primary btn-block"  id="_submit" name="_submit" type="submit">Entrar</button>
                    
                    <label class="checkbox pull-left col-md-offset-1">
                        <input type="checkbox" id="remember_me" name="_remember_me" value="on" >Me manter logado
                    </label>
                    
                </form>
                    
            </div>
                    <a href="{{ path('fos_user_resetting_request')}}" class="text-center new-account">Esqueceu sua senha?</a>
                    <a href="{{ path('fos_user_registration_register')}}" class="text-center new-account">Criar conta</a>
        </div>
    </div>
